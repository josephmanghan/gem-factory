{
  "name": "gem-factory",
  "version": "1.1.0",
  "description": "Development environment and toolkit for creating and deploying Gemini Gems with deep reasoning and knowledge-driven architecture",
  "keywords": [
    "ai-agents",
    "gems",
    "gemini",
    "prompt-engineering",
    "context-engineering",
    "style-guide"
  ],
  "repository": {
    "type": "git",
    "url": "gem-factory"
  },
  "author": "Joseph Manghan",
  "type": "module",
  "scripts": {
    "check": "npm run format:check && npm run lint",
    "deploy:target": "./scripts/deployment-handler.sh",
    "fix": "npm run format && npm run lint:fix",
    "format": "prettier --write \"**/*.{js,cjs,mjs,json,md,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,cjs,mjs,json,md,yaml}\"",
    "lint": "eslint . --max-warnings=0",
    "lint:csv": "node parse-csv.js",
    "lint:fix": "eslint . --fix",
    "pr": "npm run lint && npm run lint:csv && npm run format"
  },
  "devDependencies": {
    "csv-parse": "^6.1.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-yml": "^1.19.0",
    "fast-glob": "^3.3.3",
    "prettier": "^3.6.2",
    "prettier-plugin-packagejson": "^2.5.19",
    "yaml-eslint-parser": "^1.3.0"
  }
}
